<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="739.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mailclient.controller.MainFormController">
   <children>
      <AnchorPane prefWidth="190.0">
         <children>
            <ListView fx:id="foldersListView" onMouseClicked="#handleFoldersListViewClick" prefHeight="195.0" prefWidth="270.0" />
         </children>
      </AnchorPane>
      <ListView fx:id="accountsListView" layoutY="256.0" onMouseClicked="#handleOnAccountsListViewClick" prefHeight="405.0" prefWidth="270.0" />
      <AnchorPane fx:id="mainPane" layoutX="270.0">

      </AnchorPane>
      <Button fx:id="addAccountButton" defaultButton="true" layoutX="55.0" layoutY="667.0" onMouseClicked="#handleOnAddAccountButtonClick" prefHeight="10.0" prefWidth="161.0" text="Добавить аккаунт">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="deleteAccountButton" layoutX="55.0" layoutY="701.0" onMouseClicked="#handleOnDeleteAccountButtonClick" prefHeight="18.0" prefWidth="161.0" text="Удалить аккаунт">
         <font>
            <Font size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
